<title>Home for Sale</title>
<%= error_messages_for :property %>

<% form_for :property, :url => {:controller=>"properties", :action=>"create"},:html => { :multipart => true } do |f| -%>
  <div style="width:100%;">
    <div style="border:1px solid #c0c0c0;float:left;width:59%;height:100%">
      <div style="width:100%;background-color:#e6e6e6">
       
          <strong>Add Property Listing</strong>
        
      </div>
    
       <br/>
        <div class="caption">
        List Price:</div>
        <div class="textfieldholder"><%= f.text_field :price, :class=>"textfield" %>
        </div>
        <br/>
          <br/>
        <div class="caption">
        SQF:
        </div>

      <div class="textfieldholder"> <%= f.text_field :sqf, :class=>"textfield" %>
      </div>
      <br/>
        <br/>
